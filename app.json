{
  "name": "Baka Bot Clone",
  "description": "A telegram bot clone with Economy, Killing, and Sassy features using Pyrogram.",
  "logo": "https://cdn-icons-png.flaticon.com/512/4712/4712109.png",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram",
    "baka"
  ],
  "repository": "https://github.com/your-username/your-repo-name",
  "stack": "heroku-24",
  "env": {
    "API_ID": {
      "description": "Get this from https://my.telegram.org (API ID)",
      "required": true
    },
    "API_HASH": {
      "description": "Get this from https://my.telegram.org (API HASH)",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Get this from @BotFather on Telegram",
      "required": true
    },
    "BOT_USERNAME": {
      "description": "Your Bot's Username without @ (e.g. BakaCloneBot)",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "basic"
    }
  }
}
